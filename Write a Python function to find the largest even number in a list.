#Q. Write a Python function to find the largest even number in a list.


def find_largest_even(numbers):
    evens = []
    for num in numbers:
        if num % 2 == 0:
            evens.append(num)

    if len(evens) == 0:
        return None
    return max(evens)

numbers = []

for i in range(5):
    num = int(input(f"Enter number {i+1}: "))
    numbers.append(num)

largest_even = find_largest_even(numbers)

if largest_even is None:
    print("No even numbers in the list.")
else:
    print("Largest even number:", largest_even)
